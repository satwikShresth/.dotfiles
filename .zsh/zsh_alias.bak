#brew stuff
alias  bi='brew install'
alias  bic='brew install --cask'
alias  bu='brew uninstall'

#cd stuff
alias  cd..='cd ..'

#l stuff
alias  l='ls -lah'
alias  la='ls -lAh'
alias  ll='ls -lh'
alias  lll='ls -ltr'
